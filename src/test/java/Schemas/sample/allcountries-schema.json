{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "object",
        "properties": {
          "common": { "type": "Christmas Island" },
          "official": { "type": "Territory of Christmas Island" }
        },
        "required": ["common", "official"]
      },
      "capital": {
        "type": ["array", "null"],
        "items": { "type": "string" }
      },
      "region": { "type": "string" },
      "subregion": { "type": "string" },
      "languages": {
        "type": ["object", "null"],
        "additionalProperties": { "type": "string" }
      },
      "flags": {
        "type": "object",
        "properties": {
          "png": { "type": "string" },
          "svg": { "type": "string" }
        },
        "required": ["png", "svg"]
      },
      "population": { "type": "integer" }
    },
    "required": ["name", "region", "flags"]
  }
}